{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.77.3",
  "pnpmVersion": "7.9.3",
  "pnpmOptions": {
    "useWorkspaces": true
  },

  "nodeSupportedVersionRange": ">=12.13.0 <13.0.0 || >=14.15.0 <15.0.0 || >=16.13.0 <17.0.0",
  "ensureConsistentVersions": true,
  "gitPolicy": {},
  "repository": {
    "url": "https://github.com/qoxop/bricking.git",
    "defaultBranch": "main",
    "defaultRemote": "origin"
  },

  "eventHooks": {
    "preRushInstall": [],
    "postRushInstall": [],
    "preRushBuild": [],
    "postRushBuild": []
  },

  "variants": [],
  "projects": [
    {
      "packageName": "@bricking/toolkit",
      "projectFolder": "packages/toolkit",
      "shouldPublish": true
    },
    {
      "packageName": "@bricking/runtime",
      "projectFolder": "packages/runtime",
      "shouldPublish": true
    },
    {
      "packageName": "@bricking/plugin-style",
      "projectFolder": "packages/plugin-style",
      "shouldPublish": true
    },
    {
      "packageName": "bricking",
      "projectFolder": "packages/bricking",
      "shouldPublish": true
    },
    {
      "packageName": "@bricking/base-builder",
      "projectFolder": "packages/base-builder",
      "shouldPublish": true
    },
    {
      "packageName": "@bricking/plugin-server",
      "projectFolder": "packages/plugin-server",
      "shouldPublish": true
    },
    {
      "packageName": "@bricking/common-config",
      "projectFolder": "packages/_common",
      "shouldPublish": false
    },
    {
      "packageName": "@bricking/todo-app-svelte",
      "projectFolder": "examples/todo-app-svelte",
      "shouldPublish": false
    },
    {
      "packageName": "@bricking/todo-app-react",
      "projectFolder": "examples/todo-app-react",
      "shouldPublish": false
    },
    {
      "packageName": "@bricking/shared-libs",
      "projectFolder": "examples/shared-libs",
      "shouldPublish": false
    }
  ]
}
